{
    "$schema": "http://json.schemastore.org/template",
    "author": "Colin Halseth",
    "classifications": [ "Kerbal Space Program", "Plugin", "C#8" ],
    "identity": "KSP.Empty",
    "name": "Empty KSP Plugin",
    "shortName": "ksp",
    "tags": {
        "language": "C#",
        "type": "project"
    },
    "symbols": {
        "pluginName": {
            "type": "parameter",
            "replaces": "${PluginName}",
            "defaultValue": "MyPlugin"
        },
        "installPath": {
            "type": "parameter",
            "replaces": "${KSP_INSTALL}",
            "defaultValue": "C:\\Program Files (x86)\\Steam\\SteamApps\\common\\Kerbal Space Program"
        }
    }
}